        "-3": {
          "title": "Creating Steps",
          "id": "31.3bfb59b01b2c11e98d1f89cee76f240c",
          "ideas": {
            "1": {
              "title": "1. EC2 created",
              "id": "32.3bfb59b01b2c11e98d1f89cee76f240c"
            },
            "2": {
              "title": "2. Elastic IP allocated",
